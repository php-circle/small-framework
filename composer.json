{
  "name": "php-circle/stack-framework",
  "description": "Straight forward PHP Framework",
  "type": "project",
  "license": "MIT",
  "authors": [],
  "require": {
    "php": "^7.3",
    "zendframework/zend-diactoros": "^2.1",
    "psr/http-server-handler": "^1.0",
    "ext-json": "*",
    "illuminate/container": "^5.8",
    "nikic/fast-route": "^1.3",
    "filp/whoops": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "roave/security-advisories": "dev-master",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "PhpCircle\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
